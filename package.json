{
  "name": "payload-plugin-masquerade",
  "version": "0.0.13",
  "private": false,
  "homepage:": "https://payloadcms.com",
  "repository": "https://github.com/manutepowa/payload-plugin-masquerade",
  "description": "Masquerade user for Payload CMS",
  "author": "manutepowa@gmail.com",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./ui": {
      "import": "./dist/ui/index.js",
      "default": "./dist/ui/index.js",
      "types": "./dist/ui/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "default": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  },
  "keywords": [
    "payload",
    "cms",
    "plugin",
    "typescript",
    "react",
    "payload-plugin"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:types && pnpm build:swc",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc --strip-leading-paths --copy-files",
    "build:types": "tsc --emitDeclarationOnly",
    "build:dev": "pnpm build --watch",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "lint": "eslint src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "prepublishOnly": "pnpm clean && pnpm build"
  },
  "peerDependencies": {
    "@payloadcms/ui": "3.0.0-beta.102",
    "next": "15.0.0-canary.104",
    "payload": "3.0.0-beta.102"
  },
  "devDependencies": {
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.28",
    "@types/jsonwebtoken": "9.0.7",
    "@types/react": "npm:types-react@19.0.0-rc.0",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "chokidar": "^4.0.1",
    "copyfiles": "2.4.1",
    "cross-env": "7.0.3",
    "dotenv": "16.4.5",
    "eslint": "9.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jsonwebtoken": "^9.0.2",
    "prettier": "3.3.2",
    "prettier-plugin-organize-imports": "3.2.4",
    "react": "19.0.0-rc-06d0b89e-20240801",
    "react-dom": "19.0.0-rc-06d0b89e-20240801",
    "rimraf": "6.0.1",
    "typescript": "5.4.5"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./ui": {
        "import": "./dist/ui/index.js",
        "default": "./dist/ui/index.js",
        "types": "./dist/ui/index.d.ts"
      },
      "./types": {
        "import": "./dist/types.js",
        "default": "./dist/types.js",
        "types": "./dist/types.d.ts"
      }
    },
    "registry": "https://registry.npmjs.org/",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  }
}
